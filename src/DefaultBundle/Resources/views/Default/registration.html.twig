{% extends 'DefaultBundle::base.html.twig' %}

{% block body %}
    <div class="container" id="partners">
        <div class="row">
            <div class="box">
                <div class="col-lg-12">
                    <hr>
                    <h2 class="intro-text text-center">
                        <strong>{{ ('subtitle') | trans({}, 'registration') }}</strong>
                    </h2>
                    <hr>
                    <p>
                        {% set mail = 'email' | trans({}, 'contact') %}
                        {{ ('text') | trans({'%mailto%':'<a href=\"mailto:'~mail~'\">'~mail~'</a>'}, 'registration') | raw | nl2br }}
                    </p>
                </div>
             </div>
        </div>
    </div>
{% endblock %}
